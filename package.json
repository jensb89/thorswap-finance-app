{
  "name": "multichain-app",
  "version": "0.1.0",
  "author": "skyorion427",
  "license": "MIT",
  "description": "Multichain Web App",
  "keywords": [
    "THORChain",
    "Multichain",
    "DEX",
    "Typescript"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts --max_old_space_size=4096 build",
    "build:dev": "yarn build && yarn release-storybook",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6006 -s public",
    "build-storybook": "build-storybook -s public",
    "release-storybook": "build-storybook -o ./build/storybook",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "postinstall": "patch-package",
    "generate:api": "yarn clean:api:midgard && yarn generate:api:midgard",
    "generate:api:midgard": "openapi-generator-cli generate -i https://testnet.midgard.thorchain.info/v2/swagger.json -g typescript-axios -o ./src/midgard-sdk/api --reserved-words-mappings in=in",
    "clean:api:midgard": "rm -rf ./src/midgard-sdk/api"
  },
  "dependencies": {
    "@binance-chain/javascript-sdk": "^4.1.1",
    "@devexperts/remote-data-ts": "^2.0.5",
    "@polkadot/api": "^3.8.1",
    "@react-theming/storybook-addon": "^1.0.3",
    "@reduxjs/toolkit": "^1.5.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@thorchain/asgardex-midgard": "^1.1.0",
    "@thorchain/asgardex-theme": "^0.1.1",
    "@types/react-helmet": "^6.1.0",
    "@types/react-redux": "^7.1.16",
    "@xchainjs/xchain-binance": "^4.4.2",
    "@xchainjs/xchain-bitcoin": "^0.8.2",
    "@xchainjs/xchain-client": "^0.4.0",
    "@xchainjs/xchain-cosmos": "^0.8.1",
    "@xchainjs/xchain-crypto": "^0.2.3",
    "@xchainjs/xchain-ethereum": "^0.10.3",
    "@xchainjs/xchain-litecoin": "^0.0.2",
    "@xchainjs/xchain-polkadot": "^0.4.1",
    "@xchainjs/xchain-thorchain": "^0.10.1",
    "@xchainjs/xchain-util": "^0.2.2",
    "antd": "^4.12.2",
    "axios": "^0.21.1",
    "bignumber.js": "^9.0.1",
    "chart.js": "^2.9.4",
    "copy-to-clipboard": "^3.3.1",
    "cosmos-client": "^0.39.2",
    "ethers": "^5.0.30",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "patch-package": "^6.2.2",
    "polished": "^4.1.0",
    "postinstall-postinstall": "^2.1.0",
    "query-string": "^6.14.0",
    "react": "^17.0.1",
    "react-chartjs-2": "^2.11.1",
    "react-circular-progressbar": "^2.0.3",
    "react-content-loader": "^6.0.1",
    "react-custom-scrollbars": "^4.2.1",
    "react-dom": "^17.0.1",
    "react-draggable": "^4.4.3",
    "react-feather": "^2.0.9",
    "react-file-picker": "^0.0.6",
    "react-helmet": "^6.1.0",
    "react-intl": "^5.12.1",
    "react-placeholder": "^4.1.0",
    "react-redux": "^7.2.2",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.2",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "styled-components": "^5.2.1",
    "styled-theme": "^0.3.3",
    "tiny-invariant": "^1.1.0",
    "typescript": "^4.1.2",
    "url": "^0.11.0",
    "web-vitals": "^1.0.1",
    "webfontloader": "^1.6.28"
  },
  "devDependencies": {
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@openapitools/openapi-generator-cli": "^2.1.23",
    "@storybook/addon-actions": "^6.1.17",
    "@storybook/addon-essentials": "^6.1.17",
    "@storybook/addon-links": "^6.1.17",
    "@storybook/node-logger": "^6.1.17",
    "@storybook/preset-create-react-app": "^3.1.5",
    "@storybook/react": "^6.1.17",
    "@types/bignumber.js": "^5.0.0",
    "@types/eslint": "^7.2.6",
    "@types/lodash": "^4.14.168",
    "@types/node": "12",
    "@types/react": "^17.0.0",
    "@types/react-content-loader": "^4.0.0",
    "@types/react-custom-scrollbars": "^4.0.7",
    "@types/react-dom": "^17.0.0",
    "@types/react-router-dom": "^5.1.7",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.8",
    "@types/styled-components": "^5.1.7",
    "@types/webfontloader": "^1.6.32",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-typescript": "^12.3.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-css-modules": "^2.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-watch": "^7.0.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,json,css}": [
      "prettier --single-quote --trailing-comma all --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
